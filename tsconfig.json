{
  "extends": "@quasar/app/tsconfig-preset",
  "exclude": [
    "dev",
    "dev-clean"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      // App-defined webpack aliases
      "models/*": [
        "src/modules/authentication/templates/dist/src/models/*"
      ],
      "store/*": [
        "src/store/*",
        "src/modules/authentication/templates/dist/src/store/*"
      ],
      "api/*": [
        "src/modules/authentication/templates/dist/src/api/*"
      ],
      "composables/*": [
        "src/modules/frameworks/templates/dist/src/composables/*",
        "src/modules/config/templates/dist/src/composables/*",
        "src/modules/scroll/templates/dist/src/composables/*",
        "src/modules/float-toolbar/templates/dist/src/composables/*",
        "src/modules/multi-views/templates/dist/src/composables/*",
        "src/modules/formats/templates/dist/src/composables/*",
        "src/modules/rules/templates/dist/src/composables/*",
        "src/modules/notifications/templates/dist/src/composables/*",
        "src/modules/authentication/templates/dist/src/composables/*"
      ]
    }
  }
}
