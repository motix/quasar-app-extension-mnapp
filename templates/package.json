{
  "name": "mnapp-templates",
  "version": "0.0.1",
  "description": "Templates for mnapp",
  "productName": "mnapp Templates",
  "author": "Minh Nguyen <minh.nguyen@motiteam.com>",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint -c ./eslint.config.js \"./modules/**/*.{ts,js,cjs,mjs,vue}\"",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "postinstall": "node fixQuasarAppVite.js && quasar prepare",
    "clean": "yarn format-imports modules && yarn format --log-level warn && yarn lint --fix",
    "tsc": "yarn vue-tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "quasar": "^2.17.7",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "lodash-es": "^4.17.21",
    "js-guid": "^1.0.2",
    "@automapper/core": "^8.8.1",
    "@automapper/pojos": "^8.8.1",
    "vee-validate": "^4.15.0",
    "yup": "^1.6.1",
    "vue-markdown-render": "^2.2.1",
    "apexcharts": "^4.4.0",
    "vue3-apexcharts": "^1.8.0",
    "exceljs": "^4.4.0",
    "file-saver": "^2.0.5",
    "pinia": "^3.0.1",
    "firebase": "^11.3.1",
    "firebaseui": "^6.1.0",
    "slack": "^11.0.2",
    "slack-message-parser": "^3.0.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "eslint": "^9.20.1",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "^15.15.0",
    "vue-tsc": "^2.2.2",
    "@vue/eslint-config-typescript": "^14.4.0",
    "@vue/eslint-config-prettier": "^10.2.0",
    "prettier": "^3.5.1",
    "@types/node": "^20.5.9",
    "@quasar/app-vite": "^2.1.0",
    "typescript": "^5.7.3",
    "format-imports": "^4.0.7",
    "@types/lodash-es": "^4.17.12",
    "@types/markdown-it": "^14.1.2",
    "@types/file-saver": "^2.0.7",
    "cross-env": "^7.0.3",
    "vue-component-type-helpers": "^2.2.2"
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
