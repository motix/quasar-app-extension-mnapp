{
  "extends": "@quasar/app/tsconfig-preset",
  "compilerOptions": {
    "paths": {
      // Quasar-defined webpack aliases
      "src/*": [
        "src/*"
      ],
      "app/*": [
        "*"
      ],
      "components/*": [
        "src/components/*"
      ],
      "layouts/*": [
        "src/layouts/*"
      ],
      "pages/*": [
        "src/pages/*"
      ],
      "assets/*": [
        "src/assets/*"
      ],
      "boot/*": [
        "src/boot/*"
      ],
      // App-defined webpack aliases
      "store/*": [
        "src/store/*"
      ],
      "api/*": [
        "src/api/*"
      ],
      "utils/*": [
        "src/utils/*"
      ],
      "mixins/*": [
        "src/mixins/*"
      ],
      "uses/*": [
        "src/uses/*"
      ]
    }
  }
}
