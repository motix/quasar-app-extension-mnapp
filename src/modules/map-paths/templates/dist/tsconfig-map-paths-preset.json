{
  "extends": "<% if (config.hasModule('vite')) { %>./tsconfig-vite-preset.json<% } else { %>@quasar/app/tsconfig-preset<% } %>",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      // App-defined webpack aliases
      "models/*": ["src/models/*"],
      "api/*": ["src/api/*"],
      "services/*": ["src/services/*"],
      "stores/*": ["src/stores/*"],
      "composables/*": ["src/composables/*"],
      "mixins/*": ["src/mixins/*"],
      "utils/*": ["src/utils/*"],
      // Quasar-defined webpack aliases
      "src/*": ["src/*"],
      "app/*": ["*"],
      "components/*": ["src/components/*"],
      "layouts/*": ["src/layouts/*"],
      "pages/*": ["src/pages/*"],
      "assets/*": ["src/assets/*"],
      "boot/*": ["src/boot/*"]
    }
  }
}
